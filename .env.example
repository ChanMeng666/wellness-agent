# Choose one authentication method and uncomment the relevant section

# Google AI Studio (Gemini API)
# GOOGLE_GENAI_USE_VERTEXAI=FALSE
# GOOGLE_API_KEY=your_google_api_key_here

# Google Cloud Vertex AI
# GOOGLE_GENAI_USE_VERTEXAI=TRUE
# GOOGLE_CLOUD_PROJECT=your_gcp_project_id
# GOOGLE_CLOUD_LOCATION=us-central1
# GOOGLE_CLOUD_STORAGE_BUCKET=your_gcs_bucket_name

# Google Cloud configuration
GOOGLE_CLOUD_PROJECT="your_gcp_project_id"
GOOGLE_CLOUD_LOCATION="your_gcp_location"
GOOGLE_CLOUD_STORAGE_BUCKET="your_gcs_bucket_name"

# API key for Google AI
GOOGLE_API_KEY="your_google_api_key_here"

# Demo data for local testing (remove in production)
DEMO_PROFILE_ID="your_demo_profile_id"
DEMO_ORGANIZATION_ID="your_demo_organization_id"

# Use mock services for local development without database
USE_MOCK_SERVICES="true"

# Database connection (use service account credentials)
# FIRESTORE_EMULATOR_HOST="localhost:8080"
# FIRESTORE_PROJECT_ID="your_firestore_project_id"

# Service account credentials path (set this or use GOOGLE_APPLICATION_CREDENTIALS env var)
GOOGLE_APPLICATION_CREDENTIALS="your_service_account_credentials_path.json"

# Memory System Configuration
WELLNESS_AGENT_PROFILE="your_wellness_agent_profile_path"
# WELLNESS_AGENT_PROFILE="your_wellness_agent_profile_path"
# WELLNESS_AGENT_PROFILE="your_wellness_agent_profile_path"

# Session Management
SESSION_EXPIRY_HOURS="24"
ENABLE_CONVERSATION_HISTORY="true"
MAX_CONVERSATION_HISTORY="50"

# Logging level
LOG_LEVEL="INFO" 